<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="hotelServiceImpl" class="com.example.service.HotelServiceImpl" />
    <bean id="hotelController" class="com.example.controller.HotelController" />
    <bean id="hotelHelper" class="com.example.helper.HotelHelper" />
    <bean id="hotelClientImpl" class="com.example.wsclient.HotelClient" />
    <bean id="hotelClientConfig" class="com.example.wsclient.config.SoapClientConfig" />

    <bean id="amenityServiceImpl" class="com.example.service.AmenityServiceImpl" />
    <bean id="amenityController" class="com.example.controller.AmenityController" />
    <bean id="amenityHelper" class="com.example.helper.AmenityHelper" />
    <bean id="amenityClientImpl" class="com.example.wsclient.AmenityClient" />
    <bean id="amenityClientConfig" class="com.example.wsclient.config.SoapClientConfig" />
</beans>